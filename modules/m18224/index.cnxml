                    </m:mrow>
                  </m:mstyle>
                </m:msup>
              </m:mrow>
            </m:mstyle>
            <m:mrow/>
          </m:mrow>
          <m:annotation encoding="StarMath 5.0"> size 12{ left (2y - 6 right ) rSup { size 8{2} } } {}</m:annotation>
        </m:semantics>
      </m:math>
    </equation>
